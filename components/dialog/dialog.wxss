.dialog-fog {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #000;
  opacity: 0.5;
  z-index: 9;
}
.dialog-wrapper {
  position: absolute;
  width: 460rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  min-height: 222px;
  z-index: 10;
  background: white;
  border-radius: 5px;
  text-align: center;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.absolute {
  position: absolute;
  bottom: 15px;
}
.close {
  position: absolute;
  top: -46rpx;
  right: -46rpx;
  height: 46rpx;
  width: 46rpx;
}
p,
.description {
  word-break: break-all;
}
