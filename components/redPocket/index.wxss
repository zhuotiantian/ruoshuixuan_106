.fog {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #000;
  opacity: 0.5;
  z-index: 9;
}
.redPocket_image {
  width: 438rpx;
  height: 478rpx;
  position: absolute;
  z-index: 1000;
  top: 40%;
  margin-top: -239rpx;
  left: 50%;
  margin-left: -219rpx;
}
.close_image {
  width: 78rpx;
  height: 78rpx;
  position: absolute;
  z-index: 1000;
  top: 67%;
  margin-top: -39rpx;
  left: 50%;
  margin-left: -39rpx;
}
.success {
  position: absolute;
  color: #e21414;
  top: 30%;
